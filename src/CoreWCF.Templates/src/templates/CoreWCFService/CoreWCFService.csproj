  </ItemGroup>
  <!--#endif-->
  <ItemGroup>
    <!--#if(isNetFramework)-->
    <PackageReference Include="Microsoft.AspNetCore" Version="2.1.7" />
    <!--#endif-->
    <PackageReference Include="CoreWCF.Primitives" Version="1.*" />
    <PackageReference Include="CoreWCF.Http" Version="1.*" />
  </ItemGroup>
</Project>